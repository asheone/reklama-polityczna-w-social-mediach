# Meta Ad Library API
# Get token from: https://developers.facebook.com/tools/explorer/
META_ACCESS_TOKEN=your_long_lived_access_token_here
META_APP_ID=your_app_id_here
META_APP_SECRET=your_app_secret_here

# GCP Configuration (optional - only needed for cloud deployment)
GCP_PROJECT_ID=your-gcp-project-id
GCS_BUCKET_NAME=polish-political-ads
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Local Development Settings
OUTPUT_DIR=./output
LOG_LEVEL=INFO

# BigQuery Settings (optional)
BIGQUERY_DATASET=political_ads_raw
BIGQUERY_LOCATION=EU
